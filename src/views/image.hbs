<div class="row">
  {{#each fotos}}
  <div class="col-md-4">
    <div class="card mb-4 shadow-sm">
      <img src="{{imagenURL}}" class="bd-placeholder-img card-img-top" width="100%" height="225">
      <div class="card-body">
        <p class="card-text">{{descripcion}}</p>
        <div class="d-flex justify-content-between align-items-center">
          <div class="btn-group">
          </div>
          <small class="text-muted">{{title}}</small>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>